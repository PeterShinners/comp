---
Get top weekly thumbed issues from github
https://julianhofer.eu/blog/2025/nushell/#format-table-in-markdown-format
---

!import gh from "github-api"
!import time from "core/time"


!func main (
	let after = now() - 1#week

	list-issues (repo="nushell/nushell" fields=[title url created-at reactions])
	| filter :(it.created-at >= after)
	| map :{
		(title url) = it
		thumbs-up = it.reactions | count :(it.content == #thumbs-up)
	}
	| sort {#reverse} :(it.thumbs-up)
	| head {5}
)
